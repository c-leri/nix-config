        <type>max</type>
        <ControlType>SEQUENTIAL</ControlType>
        <CoolingDevice>
          <type>Processor</type>
        </CoolingDevice>
        </TripPoint>
      </TripPoints>
    </ThermalZone>
  </ThermalZones>
  <CoolingDevices>
    <CoolingDevice>
      <Type>Processor</Type>
      <PidControl>
        <kp>0.0002</kp>
        <kd>0</kd>
        <ki>0</ki>
      </PidControl>
    </CoolingDevice>
  </CoolingDevices>
</Platform>
</ThermalConfiguration>
